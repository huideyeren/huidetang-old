{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Sid": "PublicGetObject",
          "Effect": "Allow",
          "Principal": "*",
          "Action": "s3:GetObject",
          "Resource": "arn:aws:s3:::wagtail/images/*"
      },
      {
          "Sid": "AllowUserManageBucket",
          "Effect": "Allow",
          "Principal": {
            "AWS": "arn:aws:iam::wagtail"
          },
          "Action": [
              "s3:ListBucket",
              "s3:GetBucketLocation",
              "s3:ListBucketMultipartUploads",
              "s3:ListBucketVersions"
          ],
          "Resource": "arn:aws:s3:::wagtail"
      },
      {
          "Sid": "AllowUserManageBucketObjects",
          "Effect": "Allow",
          "Principal": {
            "AWS": "arn:aws:iam::wagtail"
          },
          "Action": [
            "s3:GetObject",
            "s3:PutObject",
            "s3:DeleteObject"
        ],
          "Resource": "arn:aws:s3:::wagtail/*"
      }
  ]
}